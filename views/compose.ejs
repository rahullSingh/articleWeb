<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group row">
            <div class="col-sm-10">
                <label for="title" class="col-sm-2 col-form-label">Title</label>
                <input class="form-control" type="text" name="title" id="title" placeholder="title"></div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <label for="post" class="col-sm-2 col-form-label">Post</label>
                <textarea style="width: 1093px;height: 188px;" class="form-control" name="post" id="post" placeholder="post"></textarea>
            </div>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
    <%- include("partials/footer") -%>